[
  {
    "name": "Lock screen",
    "exec": "swaylock -f -i ~/Pictures/Wallpapers/moon.png",
    "icon": "system-lock-screen"
  },
  {
    "name": "Suspend",
    "exec": "systemctl suspend",
    "icon": "system-suspend"
  },
  {
    "name": "Logout",
    "exec": "swaymsg exit",
    "icon": "system-log-out"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  },
  {
    "name": "Hibernate",
    "exec": "systemctl hibernate",
    "icon": "system-hibernate"
  }
]
